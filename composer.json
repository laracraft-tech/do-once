{
    "name": "laracraft-tech/memoize",
    "description": "Simple PHP trait to memoize function calls.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "LaracraftTech\\Memoize\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaracraftTech\\Memoize\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "authors": [
        {
            "name": "Zacharias Creutznacher",
            "email": "zacharias.creutznacher@gmail.com"
        }
    ],
    "license": "MIT",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "spatie/once": "^3.1",
        "symfony/var-dumper": "^6.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^1.22"
    }
}
